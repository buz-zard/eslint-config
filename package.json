{
  "name": "@k.sh/eslint-config",
  "version": "0.0.0-development",
  "description": "ESLint config based on Airbnb",
  "keywords": [
    "airbnb",
    "eslint",
    "prettier",
    "react"
  ],
  "homepage": "https://github.com/buz-zard/eslint-config/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/buz-zard/eslint-config/issues"
  },
  "repository": "git@github.com:buz-zard/eslint-config.git",
  "license": "MIT",
  "author": "Karolis Å arapnickis <pastas.k@gmail.com>",
  "main": "index.js",
  "scripts": {
    "check-deps": "sh ./scripts/validate-dependencies.sh",
    "format": "prettier --write '**/*.*' && sort-package-json",
    "lint": "run-p lint:*",
    "lint:js": "eslint .",
    "lint:md": "markdownlint *.md -i node_modules",
    "qa": "run-p test lint",
    "semantic-release": "semantic-release",
    "test": "eslint --print-config . | eslint-config-prettier-check",
    "travis-deploy-once": "travis-deploy-once",
    "upgrade-interactive": "npm-check -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json}": [
      "pretty-quick --staged"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.5.1",
    "prettier": "^1.15.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0",
    "markdownlint-cli": "^0.17.0",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "pretty-quick": "^1.11.1",
    "semantic-release": "^15.13.15",
    "sort-package-json": "^1.22.1"
  },
  "engines": {
    "node": ">=8.3"
  },
  "greenkeeper": {
    "commitMessages": {
      "initialDependencies": "feat(dependency): update dependencies",
      "dependencyUpdate": "feat(dependency): update ${dependency} to version ${version}"
    },
    "ignore": [
      "eslint",
      "eslint-plugin-import",
      "eslint-plugin-jsx-a11y",
      "eslint-plugin-react"
    ]
  }
}
