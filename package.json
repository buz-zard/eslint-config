{
  "name": "@k.sh/eslint-config",
  "version": "0.0.0-development",
  "description": "ESLint config based on Airbnb",
  "main": "index.js",
  "repository": "git@github.com:buz-zard/eslint-config.git",
  "author": "Karolis Å arapnickis <pastas.k@gmail.com>",
  "homepage": "https://github.com/buz-zard/eslint-config/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/buz-zard/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "airbnb",
    "prettier",
    "react"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "lint": "run-p lint:*",
    "lint:md": "markdownlint $(find . -name '*.md' -not -path '**/node_modules/*')",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "prettier": "^1.13.7"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.0-rc.13",
    "markdownlint-cli": "^0.10.0",
    "npm-run-all": "^4.1.3",
    "travis-deploy-once": "^5.0.1",
    "semantic-release": "^15.6.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}
