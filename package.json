{
  "name": "@k.sh/eslint-config",
  "version": "0.0.0-development",
  "description": "ESLint config based on Airbnb",
  "keywords": [
    "airbnb",
    "eslint",
    "prettier",
    "react"
  ],
  "homepage": "https://github.com/buz-zard/eslint-config/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/buz-zard/eslint-config/issues"
  },
  "repository": "git@github.com:buz-zard/eslint-config.git",
  "license": "MIT",
  "author": "Karolis Å arapnickis <pastas.k@gmail.com>",
  "main": "index.js",
  "scripts": {
    "check-deps": "npm ls >/dev/null",
    "format": "prettier --write '**/*.*' && sort-package-json",
    "qa": "run-p test lint",
    "test": "eslint --print-config . | eslint-config-prettier-check",
    "lint": "run-p lint:*",
    "lint:js": "eslint .",
    "lint:md": "markdownlint *.md -i node_modules",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "update-deps": "npm-check -u"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "prettier": "^1.13.7"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.0-rc.13",
    "markdownlint-cli": "^0.13.0",
    "npm-check": "^5.8.0",
    "npm-run-all": "^4.1.3",
    "semantic-release": "^15.9.8",
    "sort-package-json": "^1.15.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  }
}
